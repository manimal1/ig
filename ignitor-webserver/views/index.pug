head
    block head
        meta(name="viewport", content="width=device-width, initial-scale=1")

        script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")

        // Latest compiled and minified CSS
        link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", crossorigin="anonymous")

        // Optional theme
        link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css", integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp", crossorigin="anonymous")

        link(rel="stylesheet", type="text/css", href="/styles/main.css")

        // Latest compiled and minified JavaScript
        script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous")

body
    nav(class="navbar navbar-default navbar-fixed-top")
        div(class="container-fluid navbar-container")
            //- Logo and toggle for mobile display
            div(class="navbar-header")
                button(type="button", class="navbar-toggle collapsed", data-toggle="collapse", data-target="#bs-example-navbar-collapse-1", aria-expanded="false")
                    span(class="sr-only") Toggle navigation
                    span(class="icon-bar")
                    span(class="icon-bar")
                    span(class="icon-bar")
                a(class="navbar-brand" href="#") LOGO

            div(class="collapse navbar-collapse", id="bs-example-navbar-collapse-1")
                ul(class="nav navbar-nav")
                    block navlinks
                        li(class="active")
                            a(href="/") Home
                            span(class="sr-only") (current)
                        li
                            a(href="/messages") Messages
                        li
                            a(href="/groups") Groups

                ul(class="nav navbar-nav navbar-right")
                    li(class="dropdown")
                        a(href="#", data-toggle="dropdown", role="button", aria-haspopup="true", aria-expanded="false", class="dropdown-toggle")
                            span
                                img(src="" id="user-image" class="img-circle")
                            span(id="Display_TinderName") $TinderName$
                            span(class="caret")
                        ul(class="dropdown-menu")
                            li
                                a(href="/settings") Settings
                            li
                                a(href="/logout") Log Out

    block filter
        div(class="js-filter-bar filter")
            div(class="filter__wrapper")
                div(class="filter__selectors--left")
                    div(class="filter__item pad-right-sm")
                        span(class="pad-right-sm") Likes left:
                        span(class="pad-left-sm pad-right-sm circle circle--small") 100
                    div(class="filter__item")
                        span(class="pad-right-sm") Super Likes:
                        span(class="pad-left-sm pad-right-sm circle circle--small") 1
                div(class="filter__selectors--middle")
                    div(class="filter__button") Distance
                    div(class="filter__button") Age
                    div(class="filter__button") Auto-like
                div(class="filter__selectors--right")
                    div(class="filter__item")
                        span(class="pad-right-sm") New Matches:
                        span(class="pad-left-sm pad-right-sm circle circle--small") 12

    block content
        div(class="js-content content container-fluid")
            div(class="row")
                div(class="js-match-content")
                    div(class="col-sm-12 col-md-6 col-lg-3 match js-match")
                        div(id="carousel-match-1", class="carousel slide match__carousel")

                            //- Wrapper for slides
                            div(class="carousel-inner" role="listbox")
                                div(class="item active")
                                    div(class="match__image-overlay")
                                    img(style="background-image: url(https://theteenwiz.files.wordpress.com/2012/11/normal_girls_wallpaper_by_mortallity_23_2.jpg);")
                                div(class="item")
                                    div(class="match__image-overlay")
                                    img(style="background-image: url(http://telegrafi.com/wp-content/uploads/2016/08/nice-girl-pic-22.jpg);")
                                div(class="item")
                                    div(class="match__image-overlay")
                                    img(style="background-image: url(http://www.mrwallpaper.com/wallpapers/beautiful-girl-hd-1280x960.jpg);")

                            //- Controls
                            a(class="left carousel-control", href="#carousel-match-1", role="button", data-slide="prev")
                                span(class="glyphicon glyphicon-chevron-left", aria-hidden="true")
                                span(class="sr-only") Previous
                            a(class="right carousel-control", href="#carousel-match-1", role="button", data-slide="next")
                                span(class="glyphicon glyphicon-chevron-right", aria-hidden="true")
                                span(class="sr-only") Next

                            //- Add match info over match image
                            div(class="match__info match__info--top")
                                div(class="match__name") Match Name
                                div(class="match__info--right")
                                    div(class="match__age") Age:
                                        span(class="mastch__age-text")
                                        span(class="match__age--number js-match-age") 24
                                    div(class="match__distance") 3km
                            div(class="match__info match__info--bottom")
                                div(class="match__actions")
                                    div(class="match__actions-icon match__actions-icon--dislike")
                                        span(class="glyphicon glyphicon-remove" aria-hidden="true")
                                    div(class="match__actions-icon match__actions-icon--superlike")
                                        span(class="glyphicon glyphicon-heart" aria-hidden="true")
                                    div(class="match__actions-icon match__actions-icon--like")
                                        span(class="glyphicon glyphicon-ok" aria-hidden="true")

